		"chai": "^4.3.0",
		"nyc": "^15.1.0"
	},
	"keywords": [
		"btree",
		"binary-tree",
		"avl-tree",
		"data-structure",
		"tree",
		"search",
		"sort",
		"es6",
		"modern",
		"class",
		"javascript",
		"balanced-tree",
		"performance",
		"logarithmic",
		"nodejs"
	],
	"engines": {
		"node": ">=8.0.0"
	},
