{
	"name": "btreenode",
	"version": "2.0.0",
	"description": "A modern, high-performance binary search tree with AVL balancing, ES6+ features, and comprehensive error handling",
	"main": "./lib/btree.js",
	"scripts": {
		"test": "mocha test/**/*.js",
		"test:watch": "mocha test/**/*.js --watch",
		"test:coverage": "nyc mocha test/btree-modern-spec.js test/avl-balancing-spec.js",
		"test:modern": "mocha test/btree-modern-spec.js",
		"test:avl": "mocha test/avl-balancing-spec.js",
		"test:errors": "mocha test/error-handling-spec.js",
		"test:performance": "mocha test/performance-spec.js",
		"test:legacy": "mocha test/btreeSpec.js",
		"test:passing": "mocha test/btree-modern-spec.js test/avl-balancing-spec.js test/error-handling-spec.js test/performance-spec.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/richhaigh/btreenode.git"
	},
